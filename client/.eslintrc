{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "react-app"
  ],
  "rules": {
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "no-return-assign": 0,
    "eol-last": 0,
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
  }
}